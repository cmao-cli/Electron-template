{
  "name": "electron-template",
  "version": "1.0.0",
  "description": "",
  "main": "./src/main-process/main.js",
  "scripts": {
    "start": "concurrently \"npm run start-renderer-dev\" \"npm run start-main-dev\"",
    "watch": "concurrently \"npm run start-renderer-dev\" \"nodemon --delay 10ms --watch ./src/main-process --exec npm run start-main-dev\"",
    "start-main-dev": "wait-on http://localhost:8080 && cross-env NODE_ENV=development electron ./src/main-process/main.js",
    "start-renderer-dev": "cross-env NODE_ENV=development yuumi serve ./src/app/index.tsx",
    "build-renderer": "cross-env NODE_ENV=production yuumi build ./src/app/index.tsx",
    "package": "npm run build-renderer && node ./scripts/build.js"
  },
  "homepage": ".",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@babel/register": "^7.7.4",
    "@mlz/lint": "^1.0.10",
    "@types/classnames": "^2.2.9",
    "@types/react": "^16.9.13",
    "@types/react-css-modules": "^4.6.2",
    "@types/react-dom": "^16.9.4",
    "@types/react-loadable": "^5.5.2",
    "@types/react-redux": "^7.1.5",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "electron": "^7.1.2",
    "electron-builder": "^21.2.0",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "qiniu": "^7.2.2",
    "yaml": "^1.7.2",
    "nodemon": "^2.0.1",
    "wait-on": "^3.3.0",
    "concurrently": "^5.0.0"
  },
  "dependencies": {
    "@mlz/axios": "^1.0.1",
    "classnames": "^2.2.6",
    "rdx-model": "0.0.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.3"
  }
}
